//计算分数加减表达式的值
//#include <iostream>
//#include<cmath>
//#include <string.h>
//#include <iomanip>
//using namespace std;
//
//int main() {
//	double sum = 0.0;
//	int  n;
//	cin >> n;
//	for (int i = 1; i <= n ; i++) {
//		sum = sum - pow(-1, i)*1.0/double(i);
//	}
